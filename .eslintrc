{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true,
        "mocha": true,
        "node": true
    },
    "rules": {
        "comma-dangle": 0,
        "consistent-return": 0,
        "indent": [
            "error",
            4
        ],
        "no-use-before-define": 0,
        "react/jsx-no-bind": 0,
        "react/prefer-stateless-function": 0
    },
    "plugins": [
        "react"
    ]
}
