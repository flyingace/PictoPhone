{
  "name": "PictoPhone",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint app test *.js",
    "lint:sass": "sass-lint --verbose",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/.bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/.bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development",
    "package": "cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saksdirect/PictoPhone.git"
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^1.0.1",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.6.0",
    "generator-react-redux-suite": "^1.0.4",
    "generator-react-suite": "^1.0.1",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.11.2",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.6.0",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "sass-lint": "^1.7.0",
    "sass-loader": "^3.2.0",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "yeoman-generator": "^0.20.2"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "axios": "^0.11.1",
    "classnames": "^2.2.5",
    "css-modules-require-hook": "^4.0.0",
    "electron-debug": "^1.0.0",
    "firebase": "^3.0.3",
    "font-awesome": "^4.6.1",
    "mongoose": "^4.4.17",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-thunk": "^2.0.1",
    "source-map-support": "^0.4.0"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
